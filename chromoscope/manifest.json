{
  "name": "USB Oscilloscope",
  "version": "0.0.5.2",
  "description": "Stm32F3-Discovery based Oscilloscope",
  "manifest_version": 2,
  "minimum_chrome_version": "43",
  "app": {
    "background": {
      "scripts": [
        "background.js"
      ]
    }
  },
  "permissions": [
    "usb",
    {
      "usbDevices": [
        {
          "vendorId": 4617,
          "productId": 57622
        }
      ]
    }
  ],
  "icons": {
    "128": "assets/icon_128.png"
  }
}
